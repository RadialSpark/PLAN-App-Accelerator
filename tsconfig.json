{
	"compilerOptions": {
		"target": "es5",
		"module": "system",
		"moduleResolution": "node",
		"watch": false,
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false,
		"outDir": "./client/javascripts/compiled/"
	},
	"compileOnSave": false,
	"exclude": [
		"node_modules",
		"typings"
	]
}
